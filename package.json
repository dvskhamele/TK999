{
  "name": "tk999-monorepo",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install": "npm install --prefix server && npm install --prefix client",
    "dev": "concurrently \"npm:dev:server\" \"npm:dev:client\"",
    "dev:server": "npm run dev --prefix server",
    "dev:client": "npm run dev --prefix client"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  }
}